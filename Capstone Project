    {
      "cell_type": "code",
      "metadata": {
        "id": "pk9TagUW_Zb1",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "def catergory(num):\n",
        "    if num<10:\n",
        "        return(\"Start of the Month\")\n",
        "    if num<20:\n",
        "        return(\"Mid of the Month\")\n",
        "    else:\n",
        "        return(\"End of the Month\")"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Ms7jXFq1APrH",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "invoice['Category of days']=invoice['Date'].apply(lambda x: catergory(x))"
      ],
      "execution_count": null,
      "outputs": []
    },
